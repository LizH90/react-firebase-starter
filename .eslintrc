{
  "extends": [
    "react-app",
    "airbnb",
    "plugin:jsx-a11y/recommended",
  ],
  "plugins": [
    "jsx-a11y"
  ],
  "rules": {
    "react/jsx-props-no-spreading": 0,
    "react/no-unescaped-entities": 0,
    "semi": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}
